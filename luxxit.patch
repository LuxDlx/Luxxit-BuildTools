+package app.qwertz.luxxitserver.server;
+
+import app.qwertz.luxxit.command.CommandManager;
+import app.qwertz.luxxit.packet.LuxPacket;
+import app.qwertz.luxxit.player.Player;
+import app.qwertz.luxxit.plugin.PluginManager;
+import app.qwertz.luxxitserver.PluginManagerImpl;
+import app.qwertz.luxxitserver.command.CommandManagerImpl;
+import app.qwertz.luxxitserver.player.PlayerImpl;
+import app.qwertz.luxxit.server.Server;
+import app.qwertz.luxxitserver.event.EventManagerImpl;
+
+import java.util.*;
+import java.util.concurrent.ConcurrentHashMap;
+
+public class ServerImpl implements Server {
+    private final EventManagerImpl eventManager = new EventManagerImpl();
+    private final CommandManagerImpl commandManager = new CommandManagerImpl();
+    private final Map<String, PlayerImpl> playersByIp = new ConcurrentHashMap<>();
+    private final Map<String, PlayerImpl> playersByName = new ConcurrentHashMap<>();
+    private final Map<String, PlayerImpl> playersByRegCode = new ConcurrentHashMap<>();
+
+    // Example: Add a player to the server
+    public void addPlayer(PlayerImpl player) {
+        playersByIp.put(player.getIp(), player);
+        playersByName.put(player.getName(), player);
+        playersByRegCode.put(player.getRegCode(), player);
+    }
+
+    public final EventManagerImpl getEventManager() {
+        return eventManager;
+    }
+
+    @Override
+    public PluginManager getPluginManager() {
+        return PluginManagerImpl.getInstance();
+    }
+
+    // Example: Remove a player from the server
+    public void removePlayer(PlayerImpl player) {
+        playersByIp.remove(player.getIp());
+        playersByName.remove(player.getName());
+        playersByRegCode.remove(player.getRegCode());
+    }
+
+    // Get all players
+    public Collection<Player> getAllPlayers() {
+        return Collections.unmodifiableCollection(playersByIp.values());
+    }
+
+    // Get player by name (case-sensitive)
+    public PlayerImpl getPlayer(String name) {
+        try {
+            return playersByName.get(name);
+        } catch (NullPointerException e) {
+            return null;
+        }
+    }
+
+    // Get player by IP
+    public PlayerImpl getPlayerByIp(String ip) {
+        try {
+            return playersByIp.get(ip);
+        } catch (NullPointerException e) {
+            return null;
+        }
+    }
