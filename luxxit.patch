+package app.qwertz.luxxitserver.command;
+
+import app.qwertz.luxxit.command.Command;
+import app.qwertz.luxxit.command.CommandManager;
+import app.qwertz.luxxit.player.Player;
+
+import java.util.*;
+
+public class CommandManagerImpl implements CommandManager {
+    private final Map<String, Command> commands = new HashMap<>();
+
+    public void registerCommand(String name, Command command) {
+        commands.put(name, command);
+    }
+
+    public boolean dispatchCommand(Player sender, String input) {
+        String[] split = input.trim().split(" ");
+        String label = split[0].replaceFirst("^/", "");
+        String[] args = Arrays.copyOfRange(split, 1, split.length);
+
+        Command command = commands.get(label.toLowerCase());
+        if (command != null) {
+            return command.execute(sender, label, args);
+        } else {
+            return false;
+        }
+    }
+
+    public Collection<Command> getCommands() {
+        return Collections.unmodifiableCollection(commands.values());
+    }
+}
