+package app.qwertz.luxxitserver;
+
+import app.qwertz.luxxitserver.command.LuxxitCommand;
+import app.qwertz.luxxit.plugin.LuxPlugin;
+import app.qwertz.luxxit.plugin.Logger;
+import app.qwertz.luxxit.plugin.PluginManager;
+import app.qwertz.luxxitserver.plugin.LoggerImpl;
+import app.qwertz.luxxitserver.server.ServerImpl;
+
+import java.io.InputStream;
+import java.util.Collection;
+import java.util.List;
+import java.util.Map;
+import java.util.jar.JarEntry;
+import java.util.jar.JarFile;
+import java.util.HashMap;
+import java.io.File;
+import java.net.URLClassLoader;
+import java.net.URL;
+
+public class PluginManagerImpl implements PluginManager {
+    private final ServerImpl server = new ServerImpl();
+    private final Map<String, LuxPlugin> plugins = new HashMap<>();
+    private static PluginManagerImpl pluginManager;
+
+    public static void init() {
+        pluginManager = new PluginManagerImpl();
+    }
+
+    public static PluginManagerImpl getInstance() {
+        return pluginManager;
+    }
+
+    public void loadPlugins() {
+        File pluginsDir = new File("plugins");
+        if (!pluginsDir.exists()) {
+            pluginsDir.mkdirs();
+        }
+        File[] files = pluginsDir.listFiles((dir, name) -> name.endsWith(".jar"));
+        if (files != null) {
+            for (File file : files) {
+                try {
+                    loadPlugin(file);
+                } catch (Exception e) {
+                    System.err.println("Failed to load plugin: " + file.getName());
+                    e.printStackTrace();
+                }
+            }
+        }
+        getServer().getCommandManager().registerCommand("luxxit", new LuxxitCommand());
+    }
+
+    public ServerImpl getServer() {
+        return server;
+    }
+
+    public void loadPlugin(File jarFile) throws Exception {
+        URLClassLoader loader = new URLClassLoader(
+                new URL[]{jarFile.toURI().toURL()},
+                getClass().getClassLoader()
+        );
+
+        PluginAttributesImpl desc = loadPluginDescription(jarFile);
+        Class<?> pluginClass = loader.loadClass(desc.getMain());
+        LuxPlugin plugin = (LuxPlugin) pluginClass.getDeclaredConstructor().newInstance();
+
+        Logger logger = new LoggerImpl(desc.getName());
+        plugin.init(logger, server, desc);
+
+        plugins.put(desc.getName(), plugin);
+        plugin.onEnable();
+    }
+
+
+    private PluginAttributesImpl loadPluginDescription(File jarFile) throws Exception {
+        try (JarFile jar = new JarFile(jarFile)) {
+            JarEntry entry = jar.getJarEntry("plugin.yml");
+            if (entry == null) {
+                throw new IllegalArgumentException("plugin.yml not found in " + jarFile.getName());
+            }
+            try (InputStream in = jar.getInputStream(entry)) {
+                return PluginAttributesImpl.fromInputStream(in);
+            }
+        }
+    }
+
+    @Override
+    public LuxPlugin getPlugin(String pluginName) {
+        return plugins.get(pluginName);
+    }
+
+    @Override
+    public Collection<LuxPlugin> getPlugins() {
+        return plugins.values();
+    }
+}
