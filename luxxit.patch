+package app.qwertz.luxxitserver.event;
+
+import app.qwertz.luxxit.event.*;
+import app.qwertz.luxxit.event.EventListener;
+
+import java.lang.reflect.Method;
+import java.util.*;
+
+public class EventManagerImpl implements EventManager {
+    // Map of event class -> List of RegisteredListener
+    private final Map<Class<?>, List<RegisteredListener>> listeners = new HashMap<>();
+
+    public void registerEvents(app.qwertz.luxxit.event.EventListener listener) {
+        for (Method method : listener.getClass().getDeclaredMethods()) {
+            if (method.isAnnotationPresent(LuxEventHandler.class)) {
+                Class<?>[] params = method.getParameterTypes();
+                if (params.length == 1 && LuxEvent.class.isAssignableFrom(params[0])) {
+                    method.setAccessible(true);
+                    LuxEventHandler annotation = method.getAnnotation(LuxEventHandler.class);
+                    EventPriority priority = annotation.priority();
+                    listeners
+                            .computeIfAbsent(params[0], k -> new ArrayList<>())
+                            .add(new RegisteredListener(listener, method, priority));
+                }
+            }
+        }
+        // Sort listeners by priority after (re-)registering
+        for (List<RegisteredListener> regs : listeners.values()) {
+            regs.sort(Comparator.comparingInt(RegisteredListener::getPriorityOrder));
+        }
+    }
+
+     public void callEvent(LuxEvent event) {
+        List<RegisteredListener> regs = listeners.get(event.getClass());
+        if (regs != null) {
+            for (RegisteredListener reg : regs) {
+                try {
+                    reg.method.invoke(reg.listener, event);
+                } catch (Exception e) {
+                    e.printStackTrace();
+                }
+            }
+        }
+    }
+
+    private static class RegisteredListener {
+        final app.qwertz.luxxit.event.EventListener listener;
+        final Method method;
+        final EventPriority priority;
+
+        RegisteredListener(EventListener listener, Method method, EventPriority priority) {
+            this.listener = listener;
+            this.method = method;
+            this.priority = priority;
+        }
+
+        int getPriorityOrder() {
+            // Lower ordinal = lower priority, so sort ascending
+            return priority.ordinal();
+        }
+    }
+}
