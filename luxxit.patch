+package app.qwertz.luxxitserver;
+
+import java.io.InputStream;
+import java.util.Map;
+
+import app.qwertz.luxxit.plugin.PluginAttributes;
+import org.yaml.snakeyaml.Yaml;
+
+public class PluginAttributesImpl implements PluginAttributes {
+    private final String name;
+    private final String version;
+    private final String main;
+    private final String description;
+    private final int apiVersion;
+
+    public PluginAttributesImpl(String name, String version, String main, String description, int apiVersion) {
+        this.name = name;
+        this.version = version;
+        this.main = main;
+        this.description = description;
+        this.apiVersion = apiVersion;
+    }
+
+    public static PluginAttributesImpl fromInputStream(InputStream in) {
+        Yaml yaml = new Yaml();
+        Map<String, Object> data = yaml.load(in);
+
+        String name = (String) data.get("name");
+        String version = String.valueOf(data.get("version"));
+        String main = (String) data.get("main");
+        String description = (String) data.getOrDefault("description", "");
+        int apiVersion = Integer.parseInt(data.getOrDefault("api-version", "1").toString());
+
+        return new PluginAttributesImpl(name, version, main, description, apiVersion);
+    }
+
+    public String getName() {
+        return name;
+    }
+
+    public String getVersion() {
+        return version;
+    }
+
+    public String getMain() {
+        return main;
+    }
+
+    public String getDescription() {
+        return description;
+    }
+
+    public int getApiVersion() {
+        return apiVersion;
+    }
+
+    @Override
+    public String toString() {
+        return String.format("PluginDescription{name='%s', version='%s', main='%s', apiVersion=%d, description='%s'}",
+                name, version, main, apiVersion, description);
+    }
+}
